{
  "transform": {
    "^.+\\.(t|j)sx?$": [
      "@swc/jest"
    ]
  },
  "setupFilesAfterEnv": [
    "<rootDir>/src/tests/testSetup.ts"
  ],
  "collectCoverageFrom": [
    "./src/**"
  ],
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/commands/",
    "<rootDir>/src/structs/"
  ],
  "verbose": true,
  "testPathIgnorePatterns": [
    "dist/"
  ],
  "preset": "@shelf/jest-mongodb",
  "extensionsToTreatAsEsm": [
    ".ts",
    ".tsx"
  ]
}